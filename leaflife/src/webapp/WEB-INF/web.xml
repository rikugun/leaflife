<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <servlet>
        <servlet-name>JSRPCService</servlet-name>
        <servlet-class>leaflife.widget.jsrpc.JSRPCService</servlet-class>
        <init-param>
        	<param-name>MaxMarshallLevel</param-name>
        	<param-value>3</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>JSRPCService</servlet-name>
        <url-pattern>/JSRPCService/*</url-pattern>
    </servlet-mapping>
</web-app>
